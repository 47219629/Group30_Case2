from fpdf import FPDF

#Create a pdf
pdf=FPDF(orientation='P',unit="mm",format='A4')

#Add a page
pdf.add_page()

#Set Font
pdf.set_font("Arial","B",16)

#Add Text
pdf.cell(40,10,"ASX:QUB - Buy Recommendation - ")
pdf.set_xy(105,10)
pdf.set_text_color(0,128,0)
pdf.cell(40,10,"8% Upside")

#Save the pdf
pdf.output("A2.pdf")

#test
